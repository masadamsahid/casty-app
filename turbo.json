{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "$DATABASE_URL",
        "$BETTER_AUTH_SECRET",
        "$BETTER_AUTH_URL",
        "$CORS_ORIGIN",
        "$GCS_BUCKET",
        "$GCS_PROJECT_ID",
        "$GCS_CLIENT_EMAIL",
        "$GCS_PRIVATE_KEY",
        "$GOOGLE_CLIENT_ID",
        "$GOOGLE_CLIENT_SECRET"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "db:migrate": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}